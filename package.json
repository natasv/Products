{
  "name": "Skeleton",
  "version": "1.0.0",
  "description": "gbksoft app with angularjs + webpack",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=dev webpack --progress --profile --colors --display-error-details --display-cached",
    "build:watch": "NODE_ENV=dev webpack --progress --profile --colors --display-error-details --display-cached --watch",
    "deploy": "NODE_ENV=live webpack --progress --profile --no-colors --display-error-details --display-cached --optimize-minimize",
    "server": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached --watch",
    "build:live": "NODE_ENV=live webpack --progress --profile --display-error-details --display-cached --optimize-minimize --no-color",
    "build:prod": "NODE_ENV=live webpack --progress --profile --display-error-details --display-cached --optimize-minimize --no-color",
    "build:test": "NODE_ENV=live webpack --progress --profile --display-error-details --display-cached --optimize-minimize --no-color",
    "build:Live": "npm run build:live",
    "build:Dev": "npm run build:dev",
    "build:Test": "npm run build:test",
    "build:Prod": "npm run build:prod",
    "start": "npm run server"
  },
  "author": "GBKSOFT Developers",
  "license": "MIT",
  "dependencies": {
    "angular": "1.5.8",
    "angular-acl": "^0.1.7",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-formly": "^8.2.1",
    "angular-material": "^1.1.1",
    "angular-mocks": "^1.5.8",
    "angular-resource": "^1.5.8",
    "angular-restmod": "^1.1.11",
    "angular-ui-bootstrap": "^1.1.0",
    "angular-ui-router": "1.0.0-beta.1",
    "angularjs-toaster": "^1.0.0",
    "api-check": "^7.5.5",
    "autoprefixer": "^6.3.6",
    "babel-loader": "^5.3.2",
    "bootstrap": "^3.3.6",
    "bootstrap-webpack": "0.0.5",
    "bower-webpack-plugin": "^0.1.9",
    "compression-webpack-plugin": "^0.2.0",
    "copy-webpack-plugin": "^2.1.1",
    "css-loader": "^0.19.0",
    "dotenv": "^2.0.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.4.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.1",
    "http-server": "^0.8.5",
    "imports-loader": "^0.6.5",
    "install": "^0.2.3",
    "jquery": "^2.2.0",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "merge": "^1.2.0",
    "ng-annotate": "^1.2.1",
    "ng-lodash": "^0.2.3",
    "ngstorage": "^0.3.11",
    "node-sass": "^3.4.2",
    "npm": "^3.3.10",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "satellizer": "^0.13.4",
    "style-loader": "^0.12.4",
    "ui-router-extras": "^0.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
